     },\n        computed: {\n            pages() {\n                let pages = []\n                this.iconList.forEach((i, idx) => {\n                    let page = Math.floor(idx / 8)\n                    if (!pages[page]) {\n                        pages[page] = []\n                    }\n                    pages[page].push(i)\n                })\n                return pages\n            }\n        }\n    }\n</script>\n<style scoped>\n    .swiper-pagiation {\n        position: relative;\n        bottom: -.3rem !important;\n        width: .22rem;\n    }\n    \n    .swiper-container {\n        padding-bottom: .4rem;\n    }\n    \n    .swiper>>>.swiper-pagination-bullet-active {\n        background: rgba(0, 175, 190, .8) !important;\n    }\n    \n    .swiper>>>.swiper-pagination-bullet {\n        width: 6px;\n        height: 6px;\n    }\n    \n    .swiper-pagiation {\n        position: absolute;\n        z-index: 100;\n        width: 100%;\n        text-align: center;\n    }\n    \n    .icons {\n        overflow: hidden;\n        padding-bottom: 11.7rem;\n        height: 0;\n    }\n    \n    .icon {\n        position: relative;\n        overflow: hidden;\n        float: left;\n        width: 25%;\n        padding-bottom: 5.4rem;\n    }\n    \n    .icon-p {\n        position: absolute;\n        left: 0;\n        right: 0;\n        bottom: .3rem;\n        margin: 0;\n        text-align: center;\n        font-size: .8rem;\n    }\n    \n    .icon-img {\n        position: absolute;\n        top: .46rem;\n        left: 0;\n        right: 0;\n        bottom: 1.4rem;\n    }\n    \n    .icon-img img {\n        display: block;\n        margin: 0 auto;\n        height: 100%;\n    }\n</style>"]}]}